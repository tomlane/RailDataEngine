<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="exceptionless" type="Exceptionless.Configuration.ExceptionlessSection, Exceptionless" />
  </configSections>
  <exceptionless apiKey="YOUR_API_KEY" />
  <connectionStrings>
    <add name="AzureWebJobsDashboard" connectionString="connectionString" />
    <add name="AzureWebJobsStorage" connectionString="connectionString" />
    <add name="test" connectionString="Data Source=(LocalDB)\v11.0;Initial Catalog=RailDataEngine;Integrated Security=SSPI" />
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.WindowsAzure.Storage" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.2.1.0" newVersion="4.2.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <add key="FeedUri" value="stomp:tcp://datafeeds.networkrail.co.uk:61618" />
    <add key="FeedUsername" value="feedUsername" />
    <add key="FeedPassword" value="feedPassword" />
    <add key="MovementFeedTopic" value="topic://TRAIN_MVT_EF_TOC" />
    <add key="ScheduleConnectionKey" value="test" />
    <add key="TrainDescriberConnectionKey" value="test" />
    <add key="TrainMovementConnectionKey" value="test" />
  </appSettings>
</configuration>